{
  "AWSEBDockerrunVersion": "2",
  "containerDefintions": [{
    "name": "taxchatbotllm",
    "image": "wolvezine24/taxchatbotllm:latest",
    "essential": true,
    "memory": 100,
    "portMappings": [{
      "containerPort": 8080,
      "hostPort": 80
    }],
    "links": ["db"]
  },
  {
    "name": "db",
    "image": "postgres",
    "essential": true,
    "memory": 100,
    "mountPoints": [{
      "containerPath": "/var/lib/postgresql/data",
      "sourceVolume": "postgres_data"
    }],
    "portMappings":[{
      "containerPort": 5432,
      "hostPort": 5432
    }]
  }],
  "volumns": [{
    "name": "postgres_data",
    "host": {
      "sourcePath": "postgres_data"
    }
  }]
}
