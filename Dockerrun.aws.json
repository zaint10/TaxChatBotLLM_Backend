{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions": [
      {
        "name": "taxchatbotllm-backend",
        "image": "wolvezine24/taxchatbotllm-backend:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 8000
          }
        ],
        "environment": [
          {
            "name": "DEBUG",
            "value": false
          }
        ]
      }
    ]
  }
  